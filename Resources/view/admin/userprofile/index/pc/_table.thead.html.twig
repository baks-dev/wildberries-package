{% trans_default_domain 'userprofile' %}


<thead class="mb-5">
<tr class="text-start text-gray-400 fw-bolder fs-7 text-uppercase gs-0">
	
	<th class="w-10px pe-2 sorting_disabled" rowspan="1" colspan="1" style="width: 25px;">
	</th>
	
	{# Чекбокс #}
	{# Чекбокс #}
	<th class="w-25px pe-2">
		<div class="form-check form-check-sm form-check-custom pb-5">
			<label>
				<input class="form-check-input" type="checkbox" onchange="checkedAll(this)" value="1">
			</label>
		</div>
	</th>
	
	<th class="pb-5 ps-5 w-75">
		{# Название профиля #}
		{{ 'user.profile.name.label'|trans }}
	</th>
	
	<th class="pb-5 ps-5 w-25">
		{# Тип профиля #}
		{{ 'user.profile.type.label'|trans }}
	</th>
	
	<th class="pb-5 ps-5">
		{# Состояние #}
		{{ 'Аккаунт'|trans }}
	</th>
	
	<th class="pb-5 ps-5">
		{# Состояние #}
		{{ 'user.profile.active.label'|trans }}
	</th>
	
	<th class="pb-5 ps-5">
		{# Статус #}
		{{ 'user.profile.status.label'|trans }}
	</th>
	
	<th class="pb-5 ps-5">
	
	</th>
</tr>
</thead>


